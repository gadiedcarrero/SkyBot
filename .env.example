# SkyBot Universe - Environment Variables
# Copy this file to .env and fill in your actual values

# cTrader API Credentials
CTRADER_CLIENT_ID=your_client_id_here
CTRADER_SECRET=your_secret_here

# cTrader Account IDs (one per bot instance)
CTRADER_ACCOUNT_ID_1=12345678
CTRADER_ACCOUNT_ID_2=87654321

# Database
DB_PASSWORD=your_secure_password_here

# Backend API (if using)
JWT_SECRET=your_jwt_secret_here
API_PORT=3000

# Telemetry
TELEMETRY_ENABLED=true
TELEMETRY_INTERVAL_SECONDS=30

# Logging
LOG_LEVEL=Information
LOG_TO_FILE=true

# SKYTRON Economy
SKYTRON_ENABLED=true
